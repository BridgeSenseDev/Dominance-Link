{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-param-reassign": "off",
        "global-require": "off",
        "import/no-dynamic-require": "off",
        "no-restricted-syntax": "off",
        "no-shadow": "off",
        "no-console": "off",
        "no-multi-str": "off"
    },
    "globals": {
        "bot": true,
        "logChannel": true,
        "guildChannel": true,
        "statusChannel": true,
        "onlineMembers": true,
        "guildChat": true,
        "onlineChannel": true,
        "membersChannel": true,
        "levelChannel": true
    }
}
