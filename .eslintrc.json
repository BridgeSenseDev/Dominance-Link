{
    "env": {
        "es2021": true,
        "node": true
    },
    "settings": {
        "import/resolver": {
          "node": {
            "extensions": [".js", ".jsx", ".ts", ".tsx"]
          }
        }
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": "off",
        "import/no-unresolved": "off",
        "no-restricted-syntax": "off",
        "no-await-in-loop": "off"
    },
    "globals": {
        "bot": true,
        "logChannel": true,
        "guildChannel": true,
        "statusChannel": true,
        "onlineMembers": true,
        "guildChat": true,
        "onlineChannel": true,
        "membersChannel": true,
        "levelChannel": true,
        "messageCache": true,
        "guildOnline": true,
        "applicationsChannel": true,
        "officerChat": true
    }
}
