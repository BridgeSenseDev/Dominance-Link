{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "requireConfigFile": false,
        "plugins": [
            "@babel/plugin-syntax-import-assertions"
          ],
        "babelOptions": {
            "parserOpts": {
                "plugins": ["importAssertions"]
            }
        }
    },
    "rules": {
        "no-param-reassign": "off",
        "global-require": "off",
        "import/no-dynamic-require": "off",
        "no-restricted-syntax": "off",
        "no-shadow": "off",
        "no-console": "off",
        "no-multi-str": "off",
        "no-constant-condition": "off",
        "linebreak-style": "off",
        "no-case-declarations": "off",
        "import/extensions": "off",
        "no-await-in-loop": "off"
    },
    "globals": {
        "bot": true,
        "logChannel": true,
        "guildChannel": true,
        "statusChannel": true,
        "onlineMembers": true,
        "guildChat": true,
        "onlineChannel": true,
        "membersChannel": true,
        "levelChannel": true,
        "messageCache": true,
        "guildOnline": true,
        "applicationsChannel": true,
        "officerChat": true
    }
}
